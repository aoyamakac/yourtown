<div class="wrapper">
    <!--  コンテンツを一旦仮置きする  -->
    <header>

    </header>
    <div class="contents">


      <p id="notice"><%= notice %></p>

      <h1>Photos</h1>

      <table>
        <thead>
          <tr>
            <th>Image</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
          <% @town.each do |town| %>
            <tr>
              <td><%= image_tag town.image.to_s %></td>
            </tr>
            <%= link_to "削除する", town_path(town.id), method: :delete %>
          <% end %>
        </tbody>
      </table>

      <br>
      <%= link_to "新規作成", towns_new_path %>
    </div>
    <footer>
          <p>Copyright © ○○○○ All Rights Reserved.</p>
    </footer>
</div>
